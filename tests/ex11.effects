Module 'SizedString':
  Record 't':
    ([ ('name', Type ('string')); ('size', Type ('Z')) ])

(.,
  Value (non_rec, 'r', [ ], [ ], Some Type ('SizedString.t')) =
    Record
      ((?, Effect ([ ], .)),
        ('SizedString.name', Constant ((?, Effect ([ ], .)), String("gre"))),
        ('SizedString.size', Constant ((?, Effect ([ ], .)), Int(3)))))

(.,
  Value (non_rec, 'r'', [ ], [ ], Some Type ('SizedString.t')) =
    Record
      ((?, Effect ([ ], .)),
        ('SizedString.name', Constant ((?, Effect ([ ], .)), String("haha"))),
        ('SizedString.size', Constant ((?, Effect ([ ], .)), Int(4)))))

(.,
  Value (non_rec, 's', [ ], [ ], Some Type ('Z')) =
    Apply
      ((?, Effect ([ ], .)),
        Apply
          ((?, Effect ([ ], .)), Variable ((?, Effect ([ ], .)), 'Z.add'),
            Field
              ((?, Effect ([ ], .)), Variable ((?, Effect ([ ], .)), 'r'),
                'SizedString.size')),
        Field
          ((?, Effect ([ ], .)), Variable ((?, Effect ([ ], .)), 'r''),
            'SizedString.size')))

(. -> .,
  Value
    (non_rec, 'f', [ ], [ ('match_var_0', Type ('SizedString.t')) ],
      Some Type ('bool')) =
    Match
      ((?, Effect ([ ], .)), Variable ((?, Effect ([ ], .)), 'match_var_0'),
        [
          (Record (('SizedString.size', Int(3))),
            Constructor ((?, Effect ([ ], .)), 'true'));
          (Any, Constructor ((?, Effect ([ ], .)), 'false'))
        ]))

(.,
  Value (non_rec, 'b', [ ], [ ], Some Type ('bool')) =
    Apply
      ((?, Effect ([ ], .)), Variable ((?, Effect ([ ], . -> .)), 'f'),
        Variable ((?, Effect ([ ], .)), 'r')))

(.,
  Value (non_rec, 'b'', [ ], [ ], Some Type ('bool')) =
    Apply
      ((?, Effect ([ ], .)), Variable ((?, Effect ([ ], . -> .)), 'f'),
        Variable ((?, Effect ([ ], .)), 'r'')))

