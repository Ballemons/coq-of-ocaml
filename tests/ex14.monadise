Value
  (non_rec, 'f', [ ], [ ('n', Type ('Z')); ('b', Type ('bool')) ],
    Some Type ('Z')) =
  IfThenElse
    ((), Variable ((), 'b'),
      Apply
        ((), Apply ((), Variable ((), 'Z.add'), Variable ((), 'n')),
          Constant ((), Int(1))),
      Apply
        ((), Apply ((), Variable ((), 'Z.sub'), Variable ((), 'n')),
          Constant ((), Int(1))))
