((),
  Value (non_rec, 'n', [ ], [ ], Some Type ('Z')) =
    Match
      (?,
        Tuple
          (?,
            Constructor
              (?, 'cons', Constant (?, Int(1)),
                Constructor
                  (?, 'cons', Constant (?, Int(2)), Constructor (?, '[]'))),
            Constructor (?, 'false')),
        [
          (Tuple
            (Constructor
              ('cons', 'x',
                Constructor ('cons', Any, Constructor ('[]'))),
              Constructor ('true')), Variable (?, 'x'));
          (Tuple
            (Constructor
              ('cons', Any,
                Constructor ('cons', 'y', Constructor ('[]'))),
              Constructor ('false')), Variable (?, 'y'));
          (Any, Constant (?, Int(0)))
        ]))
